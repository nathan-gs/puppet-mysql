[mysqld]
innodb_buffer_pool_size			= <%= buffer_pool_size %>
innodb_additional_mem_pool_size	= <%= additional_pool_size %>
innodb_log_file_size			= <%= log_file_size %>
innodb_log_buffer_size			= <%= log_buffer_size %>

innodb_thread_concurrency		= <%= concurrency %>

innodb_flush_log_at_trx_commit 	= <%= flush_log_at_trx_commit %>
innodb_flush_method				= <%= flush_method %>